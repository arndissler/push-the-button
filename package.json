{
  "name": "push-the-button",
  "description": "Shiny button game running on arduino with johnny five.",
  "version": "0.1.1",
  "main": "src/index.js",
  "bin": "src/bin.js",
  "scripts": {
    "start": "node src/bin.js",
    "watch": "nodemon src/bin.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**",
    "build:docs": "jsdoc ./src -r -d ./docs -R ./README.md -P ./package.json",
    "emulate": "nodemon src/bin.js --emulate"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:sinnerschrader/push-the-button.git"
  },
  "bugs": {
    "url": "https://github.com/sinnerschrader/push-the-button/issues"
  },
  "dependencies": {
    "johnny-five": "0.11.3",
    "node-pixel": "0.9.3"
  },
  "devDependencies": {
    "eslint": "4.4.1",
    "express": "4.15.4",
    "jest": "20.0.4",
    "jsdoc": "3.5.4",
    "mock-require": "2.0.2",
    "nodemon": "1.11.0",
    "socket.io": "2.0.3"
  }
}
